{% for profile in awscli_profiles %}
{% if profile.name is defined %}
[{{ profile.name }}]
{% endif %}
{% if profile.access_key_id is defined %}
aws_access_key_id = {{ profile.access_key_id }}
{% endif %}
{% if profile.secret_access_key is defined %}
aws_secret_access_key = {{ profile.secret_access_key }}
{% endif %}

{% endfor %}
